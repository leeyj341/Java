package chap04;
//break문 연습 - break문이 선언된 블럭을 빠져나간다.
public class GuGu_Break {

	public static void main(String[] args) {
		for(int i = 2; i <= 9; i++)
		{
			if(i == 5) {
				break;
			}
			for(int j = 1; j <= 9; j++)
			{
				/*if(i == 5) {
					break;
				}*/
				System.out.print(i + " * " + j + " = " + i * j + "\t");
			}
			System.out.println();
		}
		
		/*for(int i = 1; i <= 9; i++)
		{
			for(int j = 2; j <= 9; j++)
			{
				System.out.print(j + " * " + i + " = " + i * j + "\t");
			}
			System.out.println();
		}*/

	}

}
