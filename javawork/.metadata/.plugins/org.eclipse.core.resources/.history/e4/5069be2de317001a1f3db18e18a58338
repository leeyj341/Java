package chap04;

public class ForExam03 {

	public static void main(String[] args) {
		
		int num0 = 0;
		int num1 = 0;
		int num2 = 0;
		
		for(int i = 1; i <= 100; i++)
		{
			num0 += i;
			if(i % 2 == 0)
				num2 += i;
			else
			{
				num1 += i;
			}
		}
		System.out.println();
		System.out.println();
		System.out.println();

	}

}
